<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
  <ui:with field="res" type="com.google.api.explorer.client.Resources" />
  <ui:style>
    .remove {
      cursor: pointer;
    }

    .placeholder {
      display: inline;
    }
  </ui:style>
  <g:HTMLPanel>
    <g:Image ui:field="remove" styleName="{style.remove}" resource="{res.remove}"
        title="Remove this property" />
    <g:InlineLabel ui:field="label" />:
    <g:HTMLPanel ui:field="placeholder" styleName="{style.placeholder}" />
  </g:HTMLPanel>
</ui:UiBinder>