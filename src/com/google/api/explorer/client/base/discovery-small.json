{
  "name" : "moderator",
  "version" : "v1",
  "description" : "Moderator API",
  "basePath" : "/moderator/v1",
  "rpcPath" : "/rpc",
  "methods": {
    "get": {
      "path": "/get/{param}",
      "rpcMethod": "moderator.get",
      "httpMethod": "GET",
      "parameters": {
        "param": {
          "required": false
        }
      }
    }
  },
  "resources": {
    "series": {
      "resources": {
        "my": {
          "methods": {
            "get": {
              "path": "/series/my/{seriesId}",
              "rpcMethod": "moderator.series.my.get",
              "httpMethod": "GET",
              "parameters": {
                "seriesId": {
                  "pattern": "(foo|bar)",
                  "required": false
                }
              }
            }
          }
        }
      },
      "methods": {
        "get": {
          "path": "/series/{seriesId}",
          "rpcMethod": "moderator.series.get",
          "httpMethod": "GET",
          "parameters": {
            "seriesId": {
              "pattern": "[^/]+",
              "required": true
            }
          }
        }
      }
    }
  }
}
